<% if @errors %>
  <ul>
    <% @errors.each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>
<form action="/users/new" method="post" class="login">
  <%= erb :"partials/_login_create_inputs" %>
  <button type="submit" value="Create Account">Create Account</button>
  <p>By registering, you agree to the <a href="/users/privacy">privacy policy</a> and <a href="/users/terms">terms of service</a>.</p>
</form>
